<script type="text/javascript">
    $(document).ready(function () {
        var focusTab = location.hash;
        console.log('anchor = ' + focusTab);
        if (focusTab) {
            var tablink = $('#tab-nav a[href$="' + focusTab + '"]');
            console.log('tablink.length = ' + tablink.length);
            if (tablink.length > 0) {
                var tab = tablink.closest('li');
                console.log('tab.length=' + tab.length);
                if (tab.length > 0) {
                    console.log('tab id=' + tab.attr('id'));
                    $("#" + tab.attr('id')).trigger('click');
                }
            }
        }

        // product tabs
        $("#tab-nav li").click(function (e) {
            e.preventDefault();
            console.log('click tab id=' + $(this).attr('id'));
            // active the selected tab
            if (!$(this).hasClass('active')) {
                $(this).addClass('active');
            };

            // unactive other items
            $("#tab-nav li").not($(this)).removeClass('active');

            // display corresponding tab content
            var index = $(this).index() + 1;
            var tabCount = $("#tab-nav li").length;
            for (var i = 1; i <= tabCount; i++) {
                if (i == index) {
                    $("#tab_" + i).addClass('show');
                } else {
                    $("#tab_" + i).removeClass('show');
                }
            }
        });

        $("#poll-no").click(function (e) {
            e.preventDefault();
            if (!$(this).hasClass('disabled')) {
                $('#poll-popup').show();
                $(this).addClass('disabled');
                $("#poll-yes").addClass('disabled');
            }
        });

        $("#poll-yes").click(function (e) {
            e.preventDefault();
            if (!$(this).hasClass('disabled')) {
                $('#poll-popup').show();
                $(this).addClass('disabled');
                $("#poll-no").addClass('disabled');
            }
        });

        $("#close-button").click(function (e) {
            $('#poll-popup').hide();
        });

        $("#faq-expand-all").click(function (e) {
            e.preventDefault();

            var text = $(this).html();

            if (text == "Mở tất cả") {
                $(this).html("Đóng tất cả");
                $(".expand-link").addClass('expanded');
                $(".faq-answer").addClass('show');
            }
            else {
                $(this).html("Mở tất cả");
                $(".expand-link").removeClass('expanded');
                $(".faq-answer").removeClass('show');
            }
        });

        $(".expand-link").click(function (e) {
            e.preventDefault();
            var id = $(this).attr('rel');
            if ($(this).hasClass('expanded')) {
                $(this).removeClass('expanded');
                $("#" + id).removeClass('show');
            } else {
                $(this).addClass('expanded');
                $(this).next('div').find('faq-answer').addClass('show');
                $("#" + id).addClass('show');
            }
        });
    });
    </script>
